//    "07:13:04":["M:2:1:8","M:2:229:9","M:2:229:8","M:3:375:8","M:3:191:8","M:3:12:8",],  


master="M:5:488:8"

CronExec("00 00 07 * * *", func(){RepatriateNow()})
CronExec("00 00 12 * * *", func(){RepatriateNow()})

for time, coord in timeCoord{
  func(time, coord) {
    entryID, err = CronExec(time, func() {
        for c in coord{
            raccoltaDeuMin(30000000,c,master)
            LogInfo("<Himalia-Ban> Partito Trasp da "+c)
            //SleepRandSec(4,8)
            SleepSec(5)
        }
        })
  }(time, coord)
}

CronExec("00 00 13 * * *",func(){
    oraArrivo = "07:40:00"
    //oraArrivo = "00:58:30"
    origin = NewCoordinate(5,488,8, MOON_TYPE)  
    arrive = NewCoordinate(5,488,9, MOON_TYPE)
    secRichiamo,oraRichiamo = calcoloRichiamo(origin,arrive,oraArrivo)
    fleetTotale(origin,arrive,secRichiamo,oraRichiamo)
    //fleeCivileSpedi(origin,arrive,secRichiamo,oraRichiamo)
})


